{
  "name": "@seyfoo/zoxy",
  "version": "1.0.0",
  "description": "A modern React library for building interactive user interfaces",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "test": "jest",
    "start": "tsc --watch && node dist/index.js",
    "build": "tsup src/index.ts --format cjs,esm --clean",
    "build:dts": "tsup src/index.ts --format cjs,esm --dts --clean",
    "prepare": "npm run build"
  },
  "keywords": [
    "react",
    "typescript",
    "ui",
    "components"
  ],
  "author": "brtsyf",
  "license": "ISC",
  "dependencies": {
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "immer": "^10.1.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "typescript": "^5.8.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brtsyf/zoxy.git"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/testing-library__jest-dom": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.1",
    "tsup": "^8.4.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "peerDependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  }
}
